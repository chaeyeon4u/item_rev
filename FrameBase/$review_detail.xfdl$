    <Script type="xscript5.1"><![CDATA[// event init
this.on_initEvent = function()
{
    trace("on_initEvent 실행됨");
    this.addEventHandler("onload", this.Form_Work_onload, this);
};

this.Form_Work_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	// 부모 프레임에서 전달한 인자를 안전하게 읽는다
	// 타 화면에서도 사용하므로 Form_Work_onload 함수 , var args = this.getOwnerFrame(); 내용 수정 금지
	var args = this.getOwnerFrame();

	// 부모 그리드에서 전달된 데이터로 입력 필드를 초기화
	this.edt_itemName.set_value(args.itemName || "");
	this.edt_registrantId.set_value(args.registrantId || "");
	this.edt_title.set_value(args.title || "");
	this.edt_content.set_value(args.content || "");
	this.Div00.form.edt_registDtm.set_value(args.registDtm || "");
};

// close popup
this.btn_close_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	trace("close");
	this.close();
};
]]></Script>
  </Form>
